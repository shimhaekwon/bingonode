{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "_moduleAliases": {
    "@root": ".",
    "@controllers": "controllers",
    "@routes": "routes",
    "@services": "services",
    "@models": "models",
    "@queries": "queries",
    "@utils": "utils",
