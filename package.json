    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "module-alias": "^2.3.4",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.3",
    "p-limit": "^7.3.0",
    "pug": "^3.0.2",
    "sqlite3": "^5.1.7",
    "zod": "4.3.6"
